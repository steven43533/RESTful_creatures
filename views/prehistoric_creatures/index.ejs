<ul>
    <% creatures.forEach((creatureElements, index) => { %>
        <li>
            <h1><%= creatureElements.type %></h1>

            <h3> <img src="<%= creatureElements.img_url %>" alt="" width= 250px height=250px> </h3>
            <form action="/prehistoric-creatures/<%= index %>/?_method=DELETE " method="POST">
                <input type="submit" style="padding:5px; margin: 1px" value="Delete">
            </form>
        </li>
    <% }) %>
</ul>